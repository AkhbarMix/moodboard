{
  "name": "lumiere-board",
  "private": true,
  "version": "0.0.0",
  "main": "electron-dist/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "npm run build && electron .",
    "electron:build": "tsc -p electron/tsconfig.json",
    "dist": "npm run build && npm run electron:build && electron-builder"
  },
  "build": {
    "appId": "com.mix3ddesign.moodboard",
    "productName": "MoodBoard By Mix 3D Design",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**",
      "electron-dist/**",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@google/genai": "^1.38.0",
    "adm-zip": "^0.5.16",
    "buffer": "^6.0.3",
    "firebase": "^12.8.0",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
